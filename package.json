    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.17",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  }
