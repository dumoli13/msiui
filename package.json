{
  "name": "mis-design",
  "license": "MIT",
  "version": "1.0.76",
  "author": "PT. Sat Nusapersada, Tbk - MIS Dept",
  "main": "dist/index.js",
  "types": "module",
  "module": "dist/es/index.js",
  "private": true,
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint src/ --max-warnings 0",
    "lint:fix": "eslint src/ --fix",
    "copy:css": "cpy \"src/**/*.css\" dist --parents",
    "copy:assets": "cpy \"src/assets/**/*\" dist/assets -r",
    "build:css": "npx tailwindcss -i ./src/input.css -o ./src/output.css --minify",
    "build": "yarn build:css && rimraf dist && tsc -p . && yarn copy:css && yarn copy:assets",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type:check": "tsc --project tsconfig.json --pretty",
    "export": "node scripts/export.js",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "classnames": "^2.5.1",
    "cpy-cli": "^5.0.0",
    "dayjs": "^1.11.13",
    "fs": "^0.0.1-security",
    "globals": "^16.0.0",
    "path": "^0.12.7",
    "react-pdf": "^9.2.1",
    "react-router-dom": "^7.3.0",
    "readline": "^1.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.1",
    "use-debounce": "^10.0.4"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@eslint/js": "^9.21.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.9",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "copy-webpack-plugin": "^13.0.0",
    "cpy-cli": "^5.0.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "file-loader": "^6.2.0",
    "husky": "^9.1.7",
    "inquirer": "^12.4.2",
    "lint-staged": "^15.4.3",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "shelljs": "^0.8.5",
    "style-loader": "^4.0.0",
    "tailwindcss": "3",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./index.d.ts"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,ts,jsx,tsx}": "eslint src/"
  }
}
