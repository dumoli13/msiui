    "react-router-dom": "^7.1.5",
    "readline": "^1.3.0",
    "tailwindcss-animate": "^1.0.7",
    "tslib": "^2.8.1",
    "use-debounce": "^10.0.4"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.26.3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
